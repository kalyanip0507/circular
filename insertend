#include<stdio.h>
#include<stdlib.h>

struct node{
    int data;
    struct node *next;
};
struct node *s;
void printlist(struct node *s){
    struct node *ptr =s;
    do{printf("LINKLIST=%d\n",ptr->data);
    ptr=ptr->next;
    }while(ptr!=s);
}
int main(){
    struct node *p,*q;
    char ch;
    p=(struct node*)malloc(sizeof(struct node));
    printf("Enter new node");
    scanf("%d",&p->data);
    s=p;
    
    do{
        q=(struct node*)malloc(sizeof(struct node));
    printf("Enter new node");
    scanf("%d",&q->data);
    p->next=q;
    q->next=s;
    p=q;
    


    printf("Press y to continue");
    ch=getch();
    }while(ch=='Y'||ch=='y');

  

printlist(s);
   

    return 0;
}
